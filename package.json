{
  "name": "uniform-context-next-contentful",
  "version": "16.1.0",
  "private": true,
  "scripts": {
    "build": "run-s generate:slug-page generate:manifest build:next",
    "dev:next": "next dev --port 5951",
    "build:next": "next build",
    "dev": "run-s generate:manifest dev:next",
    "experiment:build": "run-s generate:manifest build:next",
    "start": "next start --port 5951",
    "experiment:lint": "next lint",
    "generate:slug-page": "node ./scripts/copySlugPage.js",
    "generate:manifest": "uniform context manifest download --output ./lib/manifest.json",
    "generate:types": "contentful-typescript-codegen --output @types/generated/contentful.d.ts",
    "push:contentful": "node ./scripts/contentful-import.js",
    "push:context:signals": "uniform context signal push ./data/context/signals",
    "push:context:enrichments": "uniform context enrichment push ./data/context/enrichments",
    "push:context:aggregates": "uniform context aggregate push ./data/context/aggregates",
    "push:context:quirks": "uniform context quirk push ./data/context/quirks",
    "push:context:tests": "uniform context test push ./data/context/tests",
    "push:context": "run-s push:context:signals push:context:enrichments push:context:quirks push:context:tests push:context:aggregates",
    "pull:context:signals": "uniform context signal pull ./data/context/signals",
    "pull:context:enrichments": "uniform context enrichment pull ./data/context/enrichments",
    "pull:context:aggregates": "uniform context aggregate pull ./data/context/aggregates",
    "pull:context:quirks": "uniform context quirk pull ./data/context/quirks",
    "pull:context:tests": "uniform context test pull ./data/context/tests",
    "pull:context": "run-s pull:context:signals pull:context:enrichments pull:context:quirks pull:context:tests pull:context:aggregates"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "15.22.7",
    "@contentful/rich-text-types": "16.8.1",
    "@react-hook/window-scroll": "1.3.0",
    "@uniformdev/context": "^19.170.1",
    "@uniformdev/context-devtools": "^19.170.1",
    "@uniformdev/context-edge": "^19.170.1",
    "@uniformdev/context-edge-vercel": "^19.170.1",
    "@uniformdev/context-next": "^19.170.1",
    "@uniformdev/context-react": "^19.170.1",
    "buffer": "^6.0.3",
    "classnames": "2.3.3",
    "contentful": "9.3.6",
    "cookie": "0.6.0",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/cookie": "0.5.1",
    "@types/node": "17.0.35",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@uniformdev/cli": "^19.170.1",
    "autoprefixer": "10.4.7",
    "contentful-import": "9.4.56",
    "contentful-management": "10.46.4",
    "contentful-typescript-codegen": "3.4.0",
    "cross-env": "7.0.3",
    "dotenv": "16.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "postcss-preset-env": "7.6.0",
    "tailwindcss": "3.1.3",
    "typescript": "4.7.4"
  }
}
